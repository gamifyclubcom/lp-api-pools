    "mongoose-fuzzy-searching": "^2.0.2",
    "mongoose-paginate-v2": "^1.3.18",
    "morgan": "^1.10.0",
    "nestjs-pino": "^1.4.0",
    "nestjs-redis": "^1.3.3",
    "p-limit": "2.3.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pino-pretty": "^4.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "slugify": "^1.5.3",
