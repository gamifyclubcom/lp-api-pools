    "@solana/web3.js": "1.21.0",
    "@types/mongoose-paginate-v2": "^1.3.9",
    "@types/passport": "^1.0.6",
    "bcrypt": "^5.0.1",
    "bn.js": "^5.2.0",
    "bn.js-types": "^1.0.1",
    "bs58": "^4.0.1",
    "buffer": "^6.0.3",
    "buffer-layout": "^1.2.1",
    "buffer-to-arraybuffer": "0.0.6",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "clean-stack": "^4.1.0",
    "crypto-hash": "^1.3.0",
    "decimal.js": "^10.3.1",
